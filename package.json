{
  "name": "app4cam-frontend",
  "version": "1.1.1-next",
  "productName": "App4Cam",
  "private": true,
  "scripts": {
    "build": "quasar build",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "dev": "quasar dev",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\"",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "prepare": "husky install",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll"
  },
  "dependencies": {
    "@quasar/extras": "1.15.6",
    "apexcharts": "3.36.3",
    "axios": "1.1.3",
    "date-fns": "2.29.3",
    "date-fns-tz": "1.3.7",
    "pinia": "2.0.25",
    "quasar": "2.10.2",
    "vue": "3.2.45",
    "vue-router": "4.1.6",
    "vue3-apexcharts": "1.4.1"
  },
  "devDependencies": {
    "@pinia/testing": "0.0.14",
    "@quasar/app-vite": "1.1.3",
    "@quasar/quasar-app-extension-testing": "2.0.5",
    "@quasar/quasar-app-extension-testing-unit-jest": "3.0.0-alpha.10",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/user-event": "14.4.3",
    "@testing-library/vue": "6.6.1",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "@vue/test-utils": "2.2.4",
    "autoprefixer": "10.4.13",
    "dotenv": "16.0.3",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.5",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-testing-library": "5.9.1",
    "eslint-plugin-vue": "9.7.0",
    "husky": "8.0.2",
    "lint-staged": "12.5.0",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4"
  }
}
