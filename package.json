{
  "name": "app4cam-frontend",
  "version": "1.1.1-next",
  "productName": "App4Cam",
  "private": true,
  "scripts": {
    "build": "quasar build",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "dev": "quasar dev",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\"",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "prepare": "husky install",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll"
  },
  "dependencies": {
    "@quasar/extras": "1.15.2",
    "apexcharts": "3.35.5",
    "axios": "0.27.2",
    "date-fns": "2.29.2",
    "date-fns-tz": "1.3.6",
    "pinia": "2.0.21",
    "quasar": "2.7.7",
    "vue": "3.2.38",
    "vue-router": "4.1.5",
    "vue3-apexcharts": "1.4.1"
  },
  "devDependencies": {
    "@pinia/testing": "0.0.14",
    "@quasar/app-vite": "1.0.6",
    "@quasar/quasar-app-extension-testing": "2.0.4",
    "@quasar/quasar-app-extension-testing-unit-jest": "3.0.0-alpha.10",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/user-event": "14.4.3",
    "@testing-library/vue": "6.6.1",
    "@types/jest": "29.0.0",
    "@types/node": "18.7.15",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "@vue/test-utils": "2.0.2",
    "autoprefixer": "10.4.8",
    "dotenv": "16.0.2",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-testing-library": "5.6.1",
    "eslint-plugin-vue": "9.4.0",
    "husky": "8.0.1",
    "lint-staged": "12.5.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4"
  }
}
